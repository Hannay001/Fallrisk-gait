{
  "name": "fallrisk_tabular_v1",
  "primary_key": "participant_id",
  "label_policy": "multi_feature_v1",
  "rows": 50000,
  "fields": {
    "participant_id": {
      "type": "string",
      "description": "Synthetic identifier",
      "example": "SYN_00001"
    },
    "age_years": {
      "type": "float",
      "units": "years",
      "range": [
        55.0,
        95.0
      ],
      "description": "Participant age at assessment"
    },
    "sex": {
      "type": "categorical",
      "values": [
        "Female",
        "Male"
      ],
      "description": "Self-reported sex"
    },
    "gait_speed_mps": {
      "type": "float",
      "units": "m/s",
      "range": [
        0.64,
        1.55
      ],
      "description": "Straight-walk gait speed"
    },
    "stride_length_m": {
      "type": "float",
      "units": "m",
      "range": [
        0.939,
        1.453
      ],
      "description": "Average stride length"
    },
    "cadence_spm": {
      "type": "float",
      "units": "steps/min",
      "range": [
        89.3,
        128.9
      ],
      "description": "Step cadence during walk"
    },
    "stride_time_var": {
      "type": "float",
      "units": "s (std dev)",
      "range": [
        0.008,
        0.05
      ],
      "description": "Stride time variability (standard deviation)"
    },
    "double_support_pct": {
      "type": "float",
      "units": "% of gait cycle",
      "range": [
        12.0,
        43.77
      ],
      "description": "Fraction of the gait cycle spent in double support"
    },
    "symmetry_index": {
      "type": "float",
      "units": "unitless",
      "range": [
        0.0,
        0.152
      ],
      "description": "Gait symmetry index (higher = more asymmetry)"
    },
    "turn_time_s": {
      "type": "float",
      "units": "seconds",
      "range": [
        1.2,
        3.49
      ],
      "description": "180Â° turn segment duration in the TUG"
    },
    "sit_to_stand_s": {
      "type": "float",
      "units": "seconds",
      "range": [
        1.2,
        2.674
      ],
      "description": "Sit-to-stand subtask duration"
    },
    "stand_to_sit_s": {
      "type": "float",
      "units": "seconds",
      "range": [
        1.0,
        2.867
      ],
      "description": "Stand-to-sit subtask duration"
    },
    "tug_seconds": {
      "type": "float",
      "units": "seconds",
      "range": [
        6.002,
        13.615
      ],
      "description": "Timed Up & Go total duration"
    },
    "policy_a_risk": {
      "type": "categorical",
      "values": [
        "high",
        "low",
        "moderate"
      ],
      "description": "Policy A (TUG thresholds) risk bucket"
    },
    "policy_b_risk": {
      "type": "categorical",
      "values": [
        "high",
        "low",
        "moderate"
      ],
      "description": "Policy B (multi-feature) risk bucket"
    },
    "fall_risk": {
      "type": "categorical",
      "values": [
        "high",
        "low",
        "moderate"
      ],
      "description": "Final fall-risk label after combining policies"
    },
    "label_high_fall_risk": {
      "type": "binary",
      "values": [
        "0",
        "1"
      ],
      "description": "Binary indicator derived from fall_risk == 'high'"
    },
    "policy_a_trigger": {
      "type": "categorical",
      "values": [
        "False",
        "True"
      ],
      "description": "Whether Policy A thresholds were exceeded"
    },
    "policy_a_threshold_moderate": {
      "type": "float",
      "units": "seconds",
      "range": [
        11.0,
        11.0
      ],
