    },
    "policy_a_threshold_high": {
      "type": "float",
      "units": "seconds",
      "range": [
        13.5,
        13.5
      ],
      "description": "Policy A high-risk TUG cutoff"
    },
    "policy_b_trigger": {
      "type": "categorical",
      "values": [
        "False",
        "True"
      ],
      "description": "Whether Policy B multi-feature score crossed the trigger"
    },
