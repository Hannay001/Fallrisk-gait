{
  "name": "fallrisk_tabular_v1",
  "primary_key": "participant_id",
  "label_policy": "multi_feature_v1",
  "rows": 50000,
  "fields": {
    "participant_id": {
      "type": "string",
      "description": "Synthetic identifier",
      "example": "SYN_00001"
    },
    "age_years": {
      "type": "float",
      "units": "years",
      "range": [
        55.0,
        95.0
      ],
      "description": "Participant age at assessment"
    },
    "sex": {
      "type": "categorical",
      "values": [
        "Female",
        "Male"
      ],
      "description": "Self-reported sex"
    },
    "bmi": {
      "type": "float",
      "units": "kg/m^2",
      "range": [
        17.0,
        39.9
      ],
      "description": "Body Mass Index derived from synthetic height and weight"
    },
    "systolic_bp": {
      "type": "float",
      "units": "mmHg",
      "range": [
        95.0,
        193.0
      ],
      "description": "Systolic blood pressure"
    },
    "gait_speed_m_s": {
      "type": "float",
      "units": "m/s",
      "range": [
        0.422,
        1.683
      ],
      "description": "Straight-walk gait speed"
    },
    "stride_length_cm": {
      "type": "float",
      "units": "cm",
      "range": [
        88.1,
        150.0
      ],
      "description": "Average stride length"
    },
    "postural_sway_cm": {
      "type": "float",
      "units": "cm",
      "range": [
        0.4,
        4.859
      ],
      "description": "Center-of-mass sway during stance"
    },
    "medication_count": {
      "type": "integer",
      "units": "medications",
      "range": [
        0,
        11
      ],
      "description": "Number of daily medications"
    },
    "chronic_conditions": {
      "type": "integer",
      "units": "conditions",
      "range": [
        0,
        7
      ],
      "description": "Count of chronic conditions"
    },
    "past_falls_6mo": {
      "type": "integer",
      "units": "falls",
      "range": [
        0,
        3
      ],
      "description": "Self-reported falls in the past 6 months"
    },
    "assistive_device": {
      "type": "categorical",
      "values": [
        "None",
        "Cane",
        "Walker"
      ],
      "description": "Primary ambulation device"
    },
    "dual_task_cost_percent": {
      "type": "float",
      "units": "% slowdown",
      "range": [
        0.0,
        34.52
      ],
      "description": "Percent increase in TUG time under dual-task load"
    },
    "fear_of_falling_score": {
      "type": "integer",
      "units": "score (0-28)",
      "range": [
        0,
        28
      ],
      "description": "Falls Efficacy Score surrogate"
    },
    "muscle_strength_score": {
      "type": "float",
      "units": "score (0-100)",
      "range": [
        40.1,
        94.8
      ],
      "description": "Composite lower-body strength score"
    },
    "reaction_time_ms": {
      "type": "float",
      "units": "ms",
      "range": [
        377.7,
        885.7
      ],
      "description": "Choice reaction time"
    },
    "tug_seconds": {
      "type": "float",
      "units": "seconds",
      "range": [
        6.2,
        18.674
      ],
      "description": "Timed Up & Go total duration"
    },
    "label_high_fall_risk": {
      "type": "binary",
      "values": [
        0,
        1
      ],
      "description": "Binary high fall-risk indicator from multi_feature_v1 policy"
    },
    "label_risk_level": {
      "type": "categorical",
      "values": [
        "low",
        "moderate",
        "high"
      ],
      "description": "Three-level risk label derived from multi_feature_v1 policy"
    }
  },
  "policy_thresholds": {
    "multi_feature_v1": {
      "high_risk": [
        {
          "feature": "tug_seconds",
          "direction": "high",
          "cutoff": 13.5
        },
        {
          "feature": "gait_speed_m_s",
          "direction": "low",
          "cutoff": 0.8
        },
        {
          "feature": "past_falls_6mo",
          "direction": "high",
          "cutoff": 1
        },
        {
          "feature": "dual_task_cost_percent",
          "direction": "high",
          "cutoff": 22.0
        },
        {
          "feature": "assistive_device",
          "condition": "not 'None'"
        }
      ],
      "moderate_risk": [
        {
          "feature": "tug_seconds",
          "direction": "high",
          "cutoff": 11.2
        },
        {
          "feature": "fear_of_falling_score",
          "direction": "high",
          "cutoff": 16
        },
        {
          "feature": "medication_count",
          "direction": "high",
          "cutoff": 6
        },
        {
          "feature": "chronic_conditions",
          "direction": "high",
          "cutoff": 3
        }
      ]
    }
  },
